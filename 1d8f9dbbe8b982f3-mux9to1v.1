module top_module( 
    input [15:0] a, b, c, d, e, f, g, h, i,
    input [3:0] sel,
    output [15:0] out );
    
    // Combo logic
    always @(*) begin
       a = 16'b0;
       b = 16'b0;
       c = 16'b0;
       d = 16'b0;
       e = 16'b0;
       f = 16'b0;
       g = 16'b0;
       h = 16'b0;
       i = 16'b0;
        
        case(sel) 
            0 : out = a;
            1 : out = b;
            2 : out = c;
            3 : out = d;
            4 : out = e;
            5 : out = f;
            6 : out = g;
            7 : out = h;
            8 : out = i;
            default : out = {16{1'b1}};
        endcase
    end
endmodule
