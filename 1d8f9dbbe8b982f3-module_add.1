module top_module(
    input [31:0] a,
    input [31:0] b,
    output [31:0] sum
);
    
    wire [15:0] sum1, sum2;
    
    //Instances
    add16 inst1(.a(a[15:0]), .b(b[15:0]), .sum(sum1));
    add16 inst2(.a(a[31:16]), .b(b[31:16]), .sum(sum2));
       
    assign sum = {sum2, sum1};
endmodule
